<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dgit.mapper.RegistrationStatusMapper">


	 <resultMap type="RegistrationStatus" id="RegistrationStatusResult">
		<id property="rsNo" column="rs_no"/>
		<result property="status" column="status"/>
	</resultMap>

	<select id="selectOneRegistrationStatus" parameterType="int">
		select * from registration_status where rs_no=#{rsNo}
	</select>
	<select id="selectAllRegistrationStatus">
		select * from registration_status
	</select>

	
</mapper>